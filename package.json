{
  "name": "ultimate-resume-bilder",
  "version": "1.0.0",
  "author": "Mohitul Islam | Frontend engineer",
  "license": "ISC",
  "description": "This is ultimate editions for all whose need to make a free resume or cv",
  "main": "dist/index.js",
  "scripts": {
    "dev": "ts-node-dev -r dotenv/config -r tsconfig-paths/register ./src/index.ts",
    "build": "tsc && tsc-alias",
    "start": "node dist/index.js",
    "build:start": "npm run build && npm run start",
    "test": "jasmine",
    "test:coverage": "nyc jasmine",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "docker:build": "docker build -t ultimate-resume-builder .",
    "docker:run": "docker run -p 4321:1234 ultimate-resume-builder",
    "audit": "npm audit",
    "audit:fix": "npm audit fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mohit838/ultimate-resume-builder.git"
  },
  "dependencies": {
    "bcrypt": "^5.1.1",
    "chalk": "^5.4.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^5.1.0",
    "express-rate-limit": "^7.5.0",
    "express-validator": "^7.2.1",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.13.2",
    "morgan": "^1.10.0",
    "mysql2": "^3.14.0",
    "nodemailer": "^6.10.0",
    "redis": "^4.7.0",
    "winston": "^3.17.0",
    "winston-mongodb": "^6.0.0"
  },
  "devDependencies": {
    "@trivago/prettier-plugin-sort-imports": "^5.2.2",
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.1",
    "@types/jasmine": "^5.1.7",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/morgan": "^1.9.9",
    "@types/node": "^22.14.0",
    "@types/nodemailer": "^6.4.17",
    "@typescript-eslint/eslint-plugin": "^8.29.0",
    "@typescript-eslint/parser": "^8.29.0",
    "eslint": "^9.23.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-prettier": "^5.2.6",
    "jasmine": "^5.6.0",
    "nyc": "^17.1.0",
    "prettier": "^3.5.3",
    "ts-node-dev": "^2.0.0",
    "tsc-alias": "^1.8.13",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.8.2"
  },
  "keywords": [
    "Express",
    "Node",
    "MongoDB",
    "MySql",
    "Jwt",
    "React",
    "Vite",
    "tailwindcss",
    "antd-design",
    "react-query",
    "axios",
    "refresh-token",
    "access-token",
    "role-based",
    "typescript"
  ],
  "bugs": {
    "url": "https://github.com/mohit838/ultimate-resume-builder/issues"
  },
  "homepage": "https://github.com/mohit838/ultimate-resume-builder#readme",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "engineStrict": true
}
